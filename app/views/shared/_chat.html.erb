<%= turbo_frame_tag "chat_#{chat.id}" do %>
 <div class="text-white font-semibol">
  <%= link_to chat_path(chat), class: 'font-semibold text-white text-sm', data: {turbo_frame: 'chats_controller'} do %>
  <%= chat.name %>
    <% end %>
  <%= link_to 'Edit chat', edit_chat_path(chat) %>
 </div>
<% end %>